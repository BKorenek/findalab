{
  "name": "medology/findalab",
  "description": "Lab finder component for the Medology platform.",
  "keywords": ["jquery"],
  "license": "private",
  "type": "project",
  "require-dev": {
    "behat/behat": "^3.0",
    "behat/mink-extension": "^2.0",
    "behat/mink-selenium2-driver": "dev-viewport_visibility",
    "jarnaiz/behat-junit-formatter": "^1.3",
    "medology/flexible-mink": "dev-master",
    "vlucas/phpdotenv": "^2.3"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Medology\\": "../src/",
      "features\\contexts\\": "../features/contexts/"
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/Chekote/MinkSelenium2Driver"
    }
  ]
}
